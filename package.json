{
  "name": "mvcfolderstructure",
  "displayName": "mvc-folder-structure",
  "description": "Automatically generates an MVC folder structure for Node.js projects",
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.createMVCStructure"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.createMVCStructure",
        "title": "Create MVC Folder Structure"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.66.0",
    "vsce": "^2.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Yash-oza-24/mvc-extension"
  },
  "license": "MIT",
  "icon": "folder.png"
}
